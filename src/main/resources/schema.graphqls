type Query {
  display(
  	x: Int! = 0
  	y: Int! = 0,
  	radius: Int! = 10
  	zoom: Float! = 1
  ): String!
  
  
  getMessages(
  	channel: String!
  	effectiveTs: Long!
  ): [Message]!
  
  sendMessage(
  	channel: String!
  	secret: String!
	userId: String!
	name: String!
	message: String!
  ): Boolean
}

type Message {
	effectiveTs: Long!
	value: MessageValue!
}

type MessageValue {
	userId: String!
	name: String!
	message: String!
}
